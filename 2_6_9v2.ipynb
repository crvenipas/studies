    "import numpy as np\n",
    "\n",
    "lst = [int(i) for i in input().split()]\n",
    "x = int(input())\n",
    "\n",
    "if x in lst:\n",
    "    np_lst = np.array(lst)\n",
    "    x_index = np.where(np_lst==x)\n",
    "    print(' '.join(map(str,x_index)))\n",
    "else:\n",
    "    print('Отсутствует')"
